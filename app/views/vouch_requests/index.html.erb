<div id="content">			
	<div id="contentHeader">
		<h1>Vouch Teachers</h1>
	</div> <!-- #contentHeader -->	
	<div class="container">
		<div class="grid-24">
			<div class="widget widget-table">
				<div class="widget-header">
					<h3 class="icon chart">Substitute Information</h3>		
				</div>
				<div class="widget-content">
					<table class="table table-bordered table-striped data-table">
						<thead>
							<tr>
								<th>Date Request</th>
								<th>Requested Teacher</th>
								<th>Substitute Teacher</th>
								<th>Subject Code</th>
								<th>Room #</th>
								<th>Section</th>
								<th>Type</th>
								<th>Start Time</th>
								<th>End Time</th>
								<th>Schedule</th>
								<th>Options</th>
							</tr>
						</thead>
		       			 <tbody>
							<%= render partial: 'vouch_request', collection: @vouch_requests,  as: :vouch_request %>
				        </tbody>
			        </table>
				</div> <!-- .widget-content -->
			   </div> <!-- .widget -->
			</div> <!-- .widget-content -->
		</div> <!-- .widget -->	
	</div> <!-- .grid -->			
</div>
			
			
<!-- refresh plugin search -->
<script type="text/javascript" src="/assets/jquerysearch.js"></script>


			
